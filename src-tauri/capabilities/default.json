{
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-show",
    "core:window:allow-hide",
    "core:window:allow-close",
    "core:window:allow-set-focus",
    "core:tray:default",
    "core:tray:allow-set-icon",
    "core:tray:allow-set-tooltip",
    "shell:allow-open"
  ]
}
